{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"description": "Preset to pace updates to dependencies that must not be newer than the version that ships with Visual Studio (based on the main branch of the VS repo).",
	"extends": [
		"github>microsoft/vs-renovate-presets:dotnet_packages_below(9)",
		"github>microsoft/vs-renovate-presets:vs_components"
	],
	"packageRules": [
		{
			"matchPackageNames": [
				"MessagePack",
				"MessagePack.*",
				"MessagePackAnalyzer"
			],
			"allowedVersions": "<=2.5.192",
			"groupName": "MessagePack-CSharp"
		},
		{
			"matchPackageNames": ["Newtonsoft.Json"],
			"allowedVersions": "<=13.0.3"
		}
	]
}
